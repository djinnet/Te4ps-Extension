{
  "name": "extension-te4ps",
  "version": "1.0.0",
  "description": "Connect4 minigame Twitch Extension",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js -o dist/bundle.js -t [ babelify --presets [ @babel/preset-env ] ",
    "watch": "watchify index.js -o dist/bundle.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djinnet/Te4ps-Extension.git"
  },
  "keywords": [
    "twitch",
    "extension"
  ],
  "browserify": {
    "transform": [["babelify", { "presets": ["@babel/preset-env"] }]]
  },
  "author": "Djinnet <the.gaming.djinn@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "twitch": "^3.5.6",
    "twitch-chat-client": "^3.5.8",
    "watchify": "^3.11.1"
  }
}
